<?xml version="1.0" encoding="utf-8"?>
<configuration>

    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="ToDoModel.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog" />
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog" type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL" initializeData="FileLogWriter" />
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>

    <userSettings>
        <ToDoModel.My.MySettings>
            <setting name="Prefix_People" serializeAs="String">
                <value>Tag PPL </value>
            </setting>
            <setting name="Prefix_Context" serializeAs="String">
                <value>_@</value>
            </setting>
            <setting name="Prefix_Topic" serializeAs="String">
                <value>Tag TOPIC </value>
            </setting>
            <setting name="Prefix_Project" serializeAs="String">
                <value>Tag PROJECT </value>
            </setting>
            <setting name="Prefix_Today" serializeAs="String">
                <value>Tag A Top Priority Today</value>
            </setting>
            <setting name="Prefix_Bullpin" serializeAs="String">
                <value>Tag Bullpin Priorities</value>
            </setting>
            <setting name="Prefix_KB" serializeAs="String">
                <value>Tag KB </value>
            </setting>
            <setting name="Default_Task_Length" serializeAs="String">
                <value>15</value>
            </setting>
            <setting name="MaxToDo" serializeAs="String">
                <value>0</value>
            </setting>
        </ToDoModel.My.MySettings>
    </userSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
